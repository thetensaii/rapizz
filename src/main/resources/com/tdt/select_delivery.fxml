<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.172-ea"
      fx:controller="com.tdt.controllers.SelectDeliveryManController">
    <children>
        <Label text="Livreur"/>
        <ChoiceBox fx:id="deliveryManChoice" prefWidth="150.0"/>
        <Label text="Vehicule"/>
        <ChoiceBox fx:id="vehicleChoice" prefWidth="150.0"/>
        <Button fx:id="validate" mnemonicParsing="false" text="Valider">
            <VBox.margin>
                <Insets top="20.0"/>
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
